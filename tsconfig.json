{
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"alwaysStrict": true,
		"noImplicitThis": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"strictNullChecks": true,
		"declaration": true,
		"sourceMap": false,
		"outDir": "lib",
		"traceResolution": true,
		"types": [
			"jquery",
			"googlemaps",
			"youtube"
		]
	},
	"exclude": [
		"node_modules"
	]
}
