{
	"compileOnSave": true,
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"noImplicitThis": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"strictNullChecks": true,
		"declaration": true,
		"sourceMap": true,
		"outDir": "lib",
		"traceResolution": true
	},
	"exclude": [
		"node_modules",
		"lib"
	]
}
